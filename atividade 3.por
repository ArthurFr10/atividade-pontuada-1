programa {
  funcao inicio() {
    //Declarando variáveis
    inteiro A, B, C
    //Solicitando dados
    escreva("Digite o valor de A: ")
    leia(A)

    escreva("Digite o valor de B: ")
    leia(B)

    //Calculando
    se(A == B)
    A + B
    senao
    A * B

    C = A + B
    C = A * B
   
   
   //Exibindo resultados
    escreva("C: " + C)


  }
}
